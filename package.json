{
  "name": "nina_milo",
  "version": "1.0.0",
  "description": "Nina and Milo Groomers",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:css": "node-sass sass/main.scss css/style.css",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "concat:css": "concat -o css/style.concat.css css/icon-font.css css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 5 version' css/style.concat.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css",
    "build:babel": "node_modules/.bin/babel src/index.js -w -o dist/assets/bundle.js"
  },
  "author": "Daniel",
  "license": "ISC",
  "dependencies": {
    "node-sass": "^5.0.0",
    "save-dev": "^0.0.1-security"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "autoprefixer": "^10.0.4",
    "concat": "^1.0.3",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^8.3.0",
    "run-all": "^1.0.1"
  }
}
